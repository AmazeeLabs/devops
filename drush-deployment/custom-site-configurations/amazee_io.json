{
    "servers": {
        "prod": {
            "remote-host": "backend.zh1.single.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "amazee_io",
            "root": "/var/www/amazee_io/public_html",
            "uri": "http://amazee.io",
            "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\""
        },
        "dev": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "amazee_io",
            "root": "/var/www/amazee_io/public_html",
            "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "dev"
        },
        "other-structure": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "amazee_io_other_structure",
            "root": "/var/www/amazee_io_other_structure/public_html",
            "uri": "http://$siteurl.other-structure.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "other-structure"
        }
    }
}
