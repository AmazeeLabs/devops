{
    "servers": {
        "dev": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "resourceaward_org",
            "root": "/var/www/resourceaward_org/public_html",
            "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "dev"
        },
        "dropguard": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "resourceaward_org_dropguard",
            "root": "/var/www/resourceaward_org_dropguard/public_html",
            "uri": "http://$siteurl.dropguard.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "dropguard"
        },
        "prod": {
            "remote-host": "backend.zh1.single.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "resourceaward_org",
            "root": "/var/www/resourceaward_org/public_html",
            "uri": "http://www.resourceaward.org",
            "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\""
        }
    }
}
