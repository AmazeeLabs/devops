            "remote-environment": "amazee.io",
            "remote-user": "modelgroup_com_dev",
            "root": "/var/www/modelgroup_com_dev/public_html",
            "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "dev",
            "after": {
                "deploy-update-code": [
                    "deploy_cache_css_js_task"
                ]
            }
        },
        "prod": {
            "remote-host": "backend.zh1.single.amazee.io",
