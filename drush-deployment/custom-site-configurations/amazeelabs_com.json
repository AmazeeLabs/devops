{
  "prod": {
      "remote-host": "backend.zh1.single.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "amazeelabs_com",
      "path-aliases": {
          "%drush-script": "/usr/local/bin/drush",
          "%dump-dir": "/var/www/amazeelabs_com/"
      },
      "root": "/var/www/amazeelabs_com/public_html",
      "uri": "http://$siteurl.zh1.single.amazee.io",
      "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io 2> /dev/null\""
  },
  "dev": {
      "remote-host": "dev1.compact.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "amazeelabs_com",
      "path-aliases": {
          "%drush-script": "/usr/local/bin/drush",
          "%dump-dir": "/var/www/amazeelabs_com/"
      },
      "root": "/var/www/amazeelabs_com/public_html",
      "uri": "http://$siteurl.dev1.compact.amazee.io",
      "deploy-via": "Pull",
      "branch": "dev"
  }  
}
