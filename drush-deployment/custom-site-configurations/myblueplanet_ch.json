{
  "dev": {
        "remote-host": "dev1.compact.amazee.io",
        "remote-environment": "amazee.io",
        "remote-user": "myblueplanet_ch_dev",
        "path-aliases": {
            "%drush-script": "/usr/local/bin/drush",
            "%dump-dir": "/var/www/myblueplanet_ch_dev/"
        },
        "root": "/var/www/myblueplanet_ch_dev/public_html",
        "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
        "deploy-via": "Pull",
        "branch": "dev"
    },
  "prod": {
      "remote-host": "backend.zh1.single.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "myblueplanet_ch",
      "path-aliases": {
          "%drush-script": "/usr/local/bin/drush",
          "%dump-dir": "/var/www/myblueplanet_ch/"
      },
      "root": "/var/www/myblueplanet_ch/public_html",
      "uri": "http://$siteurl.zh1.single.amazee.io",
      "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\""
  }
}
