{
    "servers": {
        "dev": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "chymne_ch_dev",
            "root": "/var/www/chymne_ch_dev/public_html",
            "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "dev",
            "after": {
                "deploy-update-code": [
                    "deploy_cache_css_js_task"
                ]
            }
        },
        "prod1": {
            "remote-host": "backend1.zh1.cluster.amazee.io",
