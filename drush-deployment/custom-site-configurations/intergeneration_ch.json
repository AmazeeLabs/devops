{
  "prod": {
    "remote-host": "backend.zh1.single.amazee.io",
    "remote-environment": "amazee.io",
    "remote-user": "intergeneration_ch",
    "path-aliases": {
      "%drush-script": "/usr/local/bin/drush",
      "%dump-dir": "/var/www/intergeneration_ch/"
    },
    "root": "/var/www/intergeneration_ch/public_html",
    "uri": "http://$siteurl.zh1.single.amazee.io",
    "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io 2> /dev/null\"",
    "deploy-via": "Pull",
    "branch": "prod",
    "before": {
      "deploy-symlink": [
        "deploy_before_deploy_symlink_tasks"
      ]
    },
    "after": {
      "deploy-symlink": [
        "deploy_cache_task",
        "deploy_apc_flush_task",
        "deploy_newrelic_task"
      ],
      "deploy-update-code": [
        "deploy_gulp_build_task"
      ]
    }
  }
}
