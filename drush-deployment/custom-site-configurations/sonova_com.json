{
    "servers": {
        "release_3.10": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "sonova_com_release_3_10",
            "path-aliases": {
                "%drush-script": "/usr/local/bin/drush",
                "%dump-dir": "/var/www/sonova_com_release_3_10/"
            },
            "root": "/var/www/sonova_com_release_3_10/public_html",
            "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "release/3.10"
        },
        "dev": {
            "remote-host": "dev1.compact.amazee.io",
            "remote-environment": "amazee.io",
            "remote-user": "sonova_com_dev",
            "path-aliases": {
                "%drush-script": "/usr/local/bin/drush",
                "%dump-dir": "/var/www/sonova_com_dev/"
            },
            "root": "/var/www/sonova_com_dev/public_html",
            "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
            "deploy-via": "Pull",
            "branch": "dev"
        },
        "prod1": {
            "remote-host": "backend1.zh1.cluster.amazee.io",
            "remote-environment": "cluster.amazee.io",
            "remote-user": "sonova_com",
            "path-aliases": {
                "%drush-script": "/usr/local/bin/drush",
                "%dump-dir": "/var/www/sonova_com/"
            },
            "root": "/var/www/sonova_com/public_html",
            "uri": "http://www.sonova.com",
            "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.cluster.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
            "branch": "prod"
        },
        "prod2": {
            "remote-host": "backend2.zh1.cluster.amazee.io",
            "remote-environment": "cluster.amazee.io",
            "remote-user": "sonova_com",
            "path-aliases": {
                "%drush-script": "/usr/local/bin/drush",
                "%dump-dir": "/var/www/sonova_com/"
            },
            "root": "/var/www/sonova_com/public_html",
            "uri": "http://www.sonova.com",
            "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.cluster.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
            "branch": "prod"
        },
        "prod3": {
            "remote-host": "backend3.zh1.cluster.amazee.io",
            "remote-environment": "cluster.amazee.io",
            "remote-user": "sonova_com",
            "path-aliases": {
                "%drush-script": "/usr/local/bin/drush",
                "%dump-dir": "/var/www/sonova_com/"
            },
            "root": "/var/www/sonova_com/public_html",
            "uri": "http://www.sonova.com",
            "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.cluster.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
            "branch": "prod"
        }
    },
    "groups": {
        "prod": [
            "prod1",
            "prod2",
            "prod3"
        ]
    }
}