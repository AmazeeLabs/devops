{
  "servers": {
    "dev": {
      "remote-host": "dev1.compact.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "fleet-portal_ch",
      "root": "/var/www/fleet-portal_ch/public_html",
      "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
      "deploy-via": "Pull",
      "branch": "dev"
    },
    "landing-pages": {
      "remote-host": "dev1.compact.amazee.io",
