{
  "servers": {
    "dev": {
      "remote-host": "dev1.compact.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "chymne_ch_dev",
      "root": "/var/www/chymne_ch_dev/public_html",
      "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
      "deploy-via": "Pull",
      "branch": "dev",
      "after": {
        "deploy-update-code": [
          "deploy_gulp_compile_task",
          "deploy_cache_css_js_task"
        ]
      }
    },
    "prod": {
      "remote-host": "backend.zh1.single.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "chymne_ch",
      "root": "/var/www/chymne_ch/public_html",
      "uri": "http://www.chymne.ch",
      "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend2.zh1.single.amazee.io $LC_AMAZEEIO_SSH_OPTIONS -o USER=amazeelabs 2> /dev/null\"",
      "after": {
        "deploy-update-code": [
          "deploy_gulp_build_task",
          "deploy_cache_css_js_task"
        ]
      }
    }
  }
}
