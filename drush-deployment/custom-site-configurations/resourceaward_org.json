{
  "staging": {
    "remote-host": "amazeedev2.nine.ch",
    "remote-user": "www-data",
    "path-aliases": {
      "%drush-script": "/home/www-data/bin/drush",
      "%dump-dir": "/home/www-data/staging.$siteurl/"
    },
    "root": "/home/www-data/staging.$siteurl",
    "uri": "http://staging.$siteurl.amazeedev2.nine.ch"
  },
  "prod": {
      "remote-host": "backend.zh1.single.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "resourceaward_org",
      "path-aliases": {
          "%drush-script": "/usr/local/bin/drush",
          "%dump-dir": "/var/www/resourceaward_org/"
      },
      "root": "/var/www/resourceaward_org/public_html",
      "uri": "http://$siteurl.zh1.single.amazee.io",
      "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
      "branch": "prod",
      "before": {
        "deploy-symlink": [
          "deploy_before_deploy_symlink_tasks"
        ]
      },
      "after": {
        "deploy-symlink": [
          "deploy_cache_task",
          "deploy_apc_flush_task",
          "deploy_newrelic_task"
        ],
        "deploy-update-code": [
          "deploy_gulp_build_task"
        ]
      }
  }
}
