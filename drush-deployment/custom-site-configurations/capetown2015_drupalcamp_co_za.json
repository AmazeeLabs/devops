{
  "dev": {
    "remote-host": "sa1.compact.amazee.io",
    "remote-environment": "amazee.io",
    "remote-user": "capetown2015_drupalcamp_dev",
    "path-aliases": {
      "%drush-script": "/usr/local/bin/drush",
      "%dump-dir": "/var/www/capetown2015_drupalcamp_dev/"
    },
    "root": "/var/www/capetown2015_drupalcamp_dev/public_html",
    "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
    "deploy-via": "Pull",
    "branch": "dev",
    "after": {
      "deploy-update-code": [
        "deploy_cache_css_js_task"
      ]
    }
  },
  "prod": {
    "remote-host": "sa1.compact.amazee.io",
    "remote-environment": "amazee.io",
    "remote-user": "capetown2015_drupalcamp_prod",
    "path-aliases": {
      "%drush-script": "/usr/local/bin/drush",
      "%dump-dir": "/var/www/capetown2015_drupalcamp_prod/"
    },
    "root": "/var/www/capetown2015_drupalcamp_prod/public_html",
    "uri": "http://$siteurl.sa1.compact.amazee.io",
    "branch": "prod",
    "before": {
      "deploy-symlink": [
        "deploy_before_deploy_symlink_tasks"
      ]
    },
    "after": {
      "deploy-symlink": [
        "deploy_cache_task",
        "deploy_apc_flush_task",
        "deploy_newrelic_task"
      ]
    }
  }
}
