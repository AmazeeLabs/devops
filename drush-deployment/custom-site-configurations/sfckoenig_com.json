{
  "www-sfckoenig": {
    "remote-host": "amazeedev2.nine.ch",
    "remote-user": "www-sfckoenig",
    "path-aliases": {
      "%drush-script": "/home/www-sfckoenig/bin/drush",
      "%dump-dir": "/home/www-sfckoenig/dev2.$siteurl/"
    },
    "root": "/home/www-sfckoenig/dev2.$siteurl",
    "uri": "http://dev2.$siteurl.amazeedev2.nine.ch"
  },
  "amazeeiodev": {
    "remote-host": "dev1.compact.amazee.io",
    "remote-environment": "amazee.io",
    "remote-user": "sfckoenig_dev",
    "path-aliases": {
        "%drush-script": "/usr/local/bin/drush",
        "%dump-dir": "/var/www/sfckoenig_dev/"
    },
    "root": "/var/www/sfckoenig_dev/public_html",
    "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
    "deploy-via": "Pull",
    "branch": "dev",
    "after": {
      "deploy-update-code": [
        "deploy_cache_css_js_task"
      ]
    }
  },
  "prod1": {
      "remote-host": "backend1.zh1.cluster.amazee.io",
      "remote-environment": "cluster.amazee.io",
      "remote-user": "sfckoenig_com",
      "path-aliases": {
          "%drush-script": "/usr/local/bin/drush",
          "%dump-dir": "/var/www/sfckoenig_com/"
      },
      "root": "/var/www/sfckoenig_com/public_html",
      "uri": "http://www.sfckoenig.com",
      "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.cluster.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
      "branch": "prod"
  },
  "prod2": {
      "remote-host": "backend2.zh1.cluster.amazee.io",
