{
  "staging": {
    "remote-host": "amazeedev1.nine.ch",
    "remote-user": "www-data",
    "path-aliases": {
      "%drush-script": "/home/www-data/bin/drush",
      "%dump-dir": "/home/www-data/staging.$siteurl/"
    },
    "root": "/home/www-data/staging.$siteurl",
    "uri": "http://staging.$siteurl.amazeedev1.nine.ch"
  },
  "prod1": {
      "remote-host": "backend1.zh1.cluster.amazee.io",
      "remote-environment": "amazee.io",
      "remote-user": "sonova_com",
      "path-aliases": {
          "%drush-script": "/usr/local/bin/drush",
          "%dump-dir": "/var/www/sonova_com/"
      },
      "root": "/var/www/sonova_com/public_html",
      "uri": "http://www.sonova.com",
      "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.cluster.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
      "branch": "prod"
  },
  "prod2": {
