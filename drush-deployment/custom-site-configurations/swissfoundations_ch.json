{
    "dev": {
        "remote-host": "dev1.compact.amazee.io",
        "remote-environment": "amazee.io",
        "remote-user": "swissfoundations_ch",
        "path-aliases": {
            "%drush-script": "/usr/local/bin/drush",
            "%dump-dir": "/var/www/swissfoundations_ch/"
        },
        "root": "/var/www/swissfoundations_ch/public_html",
        "uri": "http://$siteurl.dev.dev1.compact.amazee.io",
        "deploy-via": "Pull",
        "branch": "dev"
    },
    "prod": {
        "remote-host": "backend.zh1.single.amazee.io",
        "remote-environment": "amazee.io",
        "remote-user": "swissfoundations_ch",
        "path-aliases": {
            "%drush-script": "/usr/local/bin/drush",
            "%dump-dir": "/var/www/swissfoundations_ch/"
        },
        "root": "/var/www/swissfoundations_ch/public_html",
        "uri": "http://www.swissfoundations.ch",
        "ssh-options": "-o ProxyCommand=\"ssh -W %h:%p frontend.zh1.single.amazee.io $AMAZEEIO_SSH_OPTIONS 2> /dev/null\"",
        "branch": "prod",
        "before": {
          "deploy-symlink": [
            "deploy_before_deploy_symlink_tasks"
          ]
        },
        "after": {
          "deploy-symlink": [
            "deploy_cache_task",
            "deploy_apc_flush_task",
            "deploy_newrelic_task"
          ]
        }
    }
}
